			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>ВЫБРАТЬ
	ЖурналДокументовДенежныеДокументы.Ссылка,
	ЖурналДокументовДенежныеДокументы.Дата,
	ЖурналДокументовДенежныеДокументы.ПометкаУдаления,
	ЖурналДокументовДенежныеДокументы.Номер,
	ЖурналДокументовДенежныеДокументы.Проведен,
	ЖурналДокументовДенежныеДокументы.РучнаяКорректировка,
	ЖурналДокументовДенежныеДокументы.Поступление,
	ЖурналДокументовДенежныеДокументы.Выдача,
	ЖурналДокументовДенежныеДокументы.Валюта,
	ЖурналДокументовДенежныеДокументы.Контрагент,
	ЖурналДокументовДенежныеДокументы.ВидОперации,
	ЖурналДокументовДенежныеДокументы.Организация,
	ЖурналДокументовДенежныеДокументы.ПодразделениеОрганизации,
	ЖурналДокументовДенежныеДокументы.Ответственный,
	ЖурналДокументовДенежныеДокументы.Комментарий,
	ЖурналДокументовДенежныеДокументы.Тип,
	ВЫБОР
		КОГДА ЖурналДокументовДенежныеДокументы.РучнаяКорректировка = ИСТИНА
			ТОГДА ВЫБОР
					КОГДА ЖурналДокументовДенежныеДокументы.ПометкаУдаления = ИСТИНА
						ТОГДА 10
					КОГДА ЖурналДокументовДенежныеДокументы.Проведен = ЛОЖЬ
						ТОГДА 9
					ИНАЧЕ 8
				КОНЕЦ
		ИНАЧЕ ВЫБОР
