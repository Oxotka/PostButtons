				</UseAlways>
				<Settings xsi:type="DynamicList">
					<ManualQuery>true</ManualQuery>
					<DynamicDataRead>true</DynamicDataRead>
					<QueryText>ВЫБРАТЬ
	ДокументНачислениеПени.Ссылка,
	ДокументНачислениеПени.ПометкаУдаления,
	ДокументНачислениеПени.Номер,
	ДокументНачислениеПени.Дата,
	ДокументНачислениеПени.Проведен,
	ДокументНачислениеПени.Организация,
	ДокументНачислениеПени.ПодразделениеОрганизации,
	ДокументНачислениеПени.ВалютаДокумента КАК Валюта,
	ДокументНачислениеПени.КурсВзаиморасчетов,
	ДокументНачислениеПени.КратностьВзаиморасчетов,
	ДокументНачислениеПени.Контрагент,
	ДокументНачислениеПени.Ответственный,
	ДокументНачислениеПени.Комментарий,
	ДокументНачислениеПени.РучнаяКорректировка,
	ВЫБОР
		КОГДА ДокументНачислениеПени.РучнаяКорректировка = ИСТИНА
			ТОГДА ВЫБОР
					КОГДА ДокументНачислениеПени.ПометкаУдаления = ИСТИНА
						ТОГДА 10
					КОГДА ДокументНачислениеПени.Проведен = ЛОЖЬ
						ТОГДА 9
					ИНАЧЕ 8
				КОНЕЦ
		ИНАЧЕ ВЫБОР
				КОГДА ДокументНачислениеПени.ПометкаУдаления = ИСТИНА
					ТОГДА 2
