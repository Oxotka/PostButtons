			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>ВЫБРАТЬ
	ДокументАвансовыйОтчет.Ссылка,
	ДокументАвансовыйОтчет.ПометкаУдаления,
	ДокументАвансовыйОтчет.Номер,
	ДокументАвансовыйОтчет.Дата,
	ДокументАвансовыйОтчет.Проведен,
	ДокументАвансовыйОтчет.ВалютаДокумента,
	ДокументАвансовыйОтчет.Комментарий,
	ДокументАвансовыйОтчет.КурсДокумента,
	ДокументАвансовыйОтчет.КратностьДокумента,
	ДокументАвансовыйОтчет.Организация,
	ДокументАвансовыйОтчет.Склад,
	ДокументАвансовыйОтчет.СуммаВключаетНДС,
	ДокументАвансовыйОтчет.СуммаДокумента,
	ДокументАвансовыйОтчет.ТипЦен,
	ДокументАвансовыйОтчет.ФизЛицо,
	ДокументАвансовыйОтчет.Ответственный,
	ДокументАвансовыйОтчет.НДСВключенВСтоимость,
	ДокументАвансовыйОтчет.НазначениеАванса,
	ДокументАвансовыйОтчет.КоличествоДокументов,
	ДокументАвансовыйОтчет.КоличествоЛистов,
	ДокументАвансовыйОтчет.РучнаяКорректировка,
	ДокументАвансовыйОтчет.ПодразделениеОрганизации,
	ДокументАвансовыйОтчет.ВыданныеАвансы,
	ДокументАвансовыйОтчет.Товары,
	ДокументАвансовыйОтчет.ВозвратнаяТара,
	ДокументАвансовыйОтчет.ОплатаПоставщикам,
	ДокументАвансовыйОтчет.Прочее,
	ВЫБОР
		КОГДА ДокументАвансовыйОтчет.РучнаяКорректировка = ИСТИНА
			ТОГДА ВЫБОР
					КОГДА ДокументАвансовыйОтчет.ПометкаУдаления = ИСТИНА
						ТОГДА 10
					КОГДА ДокументАвансовыйОтчет.Проведен = ЛОЖЬ
						ТОГДА 9
					ИНАЧЕ 8
				КОНЕЦ
		ИНАЧЕ ВЫБОР
				КОГДА ДокументАвансовыйОтчет.ПометкаУдаления = ИСТИНА
					ТОГДА 2
				КОГДА ДокументАвансовыйОтчет.Проведен = ИСТИНА
					ТОГДА 1
				ИНАЧЕ 0
			КОНЕЦ
	КОНЕЦ КАК СостояниеДокумента,
	ДокументАвансовыйОтчет.МоментВремени,
	ВЫБОР
		КОГДА ДокументыСОшибкамиПроверкиКонтрагентов.Документ ЕСТЬ NULL 
			ТОГДА ЛОЖЬ
		ИНАЧЕ ИСТИНА
	КОНЕЦ КАК СодержитНекорректныхКонтрагентов,
	НаличиеПрисоединенныхФайлов.ЕстьФайлы
ИЗ
	Документ.АвансовыйОтчет КАК ДокументАвансовыйОтчет
	ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ДокументыСОшибкамиПроверкиКонтрагентов КАК ДокументыСОшибкамиПроверкиКонтрагентов
		ПО ДокументАвансовыйОтчет.Ссылка = ДокументыСОшибкамиПроверкиКонтрагентов.Документ
		И &amp;ИспользованиеПроверкиВозможно
	ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.НаличиеПрисоединенныхФайлов КАК НаличиеПрисоединенныхФайлов
		ПО НаличиеПрисоединенныхФайлов.ОбъектСФайлами = ДокументАвансовыйОтчет.Ссылка
</QueryText>
